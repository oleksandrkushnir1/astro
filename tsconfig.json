{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "types": ["@types/node", "@types/react", "@types/vite-plugin-ssr"],
    "jsxImportSource": "react",
    "allowJs": true,
    "resolveJsonModule": true
  }
}